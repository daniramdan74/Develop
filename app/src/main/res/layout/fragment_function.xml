<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.dipesan.miniatm.miniatm.view.function.FunctionFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <Button
            android:id="@+id/fragment_function_void_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/function_void"
            android:textColor="@color/colorTextIcons"
            android:textStyle="bold"
            android:theme="@style/ButtonTheme"
            />

        <Button
            android:id="@+id/fragment_function_settelment_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/function_settelment"
            android:textColor="@color/colorTextIcons"
            android:textStyle="bold"
            android:theme="@style/ButtonTheme"
            />

    </LinearLayout>

</FrameLayout>
